local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "TV HUB", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = " TV RB HUB"})

local Tab = Window:MakeTab({
	Name = "Main-Features",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Spawn Any Units"
})

Tab:AddButton({
	Name = "Secret Agent",
	Callback = function()
      		local args = {
    [1] = "Secret Agent",
    [2] = CFrame.new(game.Players.LocalPlayer.Character.Head.Position)
}

game:GetService("ReplicatedStorage").Functions.SpawnTower:InvokeServer(unpack(args))

  	end    
})

